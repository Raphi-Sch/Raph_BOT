{
    "version": "7.1.x",
    "table": {
        "authentication": {
            "fields": [
                "id",
                "client",
                "token_hash",
                "expiration",
                "note",
                "usage_type"
            ],
            "types": {
                "id": "INT(11) NULL DEFAULT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "client": "text NOT NULL",
                "token_hash": "text NOT NULL",
                "expiration": "date DEFAULT NULL",
                "note": "text DEFAULT NULL",
                "usage_type": "INT(4) DEFAULT 2"
            }
        },
        "commands": {
            "fields": [
                "id",
                "command",
                "value",
                "auto",
                "mod_only",
                "sub_only",
                "tts"
            ],
            "types": {
                "id": "INT(11) NULL DEFAULT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "command": "TEXT NULL",
                "value": "TEXT NULL",
                "auto": "BOOLEAN NOT NULL DEFAULT FALSE",
                "mod_only": "BOOLEAN NOT NULL DEFAULT FALSE",
                "sub_only": "BOOLEAN NOT NULL DEFAULT FALSE",
                "tts": "BOOLEAN NOT NULL DEFAULT FALSE"
            }
        },
        "commands_alias": {
            "fields": [
                "id",
                "alias",
                "command"
            ],
            "types": {
                "id": "INT(11) NULL DEFAULT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "alias": "text NOT NULL",
                "command": "text NOT NULL"
            }
        },
        "commands_presets": {
            "fields": [
                "id",
                "name",
                "commands"
            ],
            "types": {
                "id": "INT(11) NULL DEFAULT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "name": "text NOT NULL",
                "commands": "text NOT NULL"
            }
        },
        "commands_audio": {
            "fields": [
                "id",
                "name",
                "trigger_word",
                "file",
                "volume",
                "timeout",
                "active",
                "mod_only",
                "sub_only"
            ],
            "types": {
                "id": "int(11) NOT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "name": "text NOT NULL",
                "trigger_word": "varchar(20) NOT NULL",
                "file": "text NOT NULL",
                "volume": "float NOT NULL DEFAULT 1",
                "timeout": "int(11) NOT NULL",
                "active": "tinyint(1) NOT NULL DEFAULT 1",
                "mod_only": "tinyint(1) NOT NULL DEFAULT 0",
                "sub_only": "tinyint(1) NOT NULL DEFAULT 0"
            }
        },
        "commands_config": {
            "fields": [
                "id",
                "value",
                "type"
            ],
            "types": {
                "id": "varchar(30) NOT NULL, ADD PRIMARY KEY (`id`)",
                "value": "text NULL",
                "type": "int(4) NOT NULL"
            }
        },
        "config": {
            "fields": [
                "id",
                "value",
                "hidden",
                "type",
                "help"
            ],
            "types": {
                "id": "varchar(30) NOT NULL",
                "value": "text NOT NULL",
                "hidden": "tinyint(1) DEFAULT 0",
                "type": "tinyint(4) NOT NULL DEFAULT 0",
                "help": "text NULL"
            }
        },
        "moderator": {
            "fields": [
                "id",
                "trigger_word",
                "mod_action",
                "explanation",
                "duration",
                "reason",
                "seriousness"
            ],
            "types": {
                "id": "int(11) NOT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "trigger_word": "text NOT NULL",
                "mod_action": "tinyint(1) NOT NULL",
                "explanation": "text NOT NULL",
                "duration": "int(11) NOT NULL",
                "reason": "text NOT NULL",
                "seriousness": "INT NOT NULL DEFAULT 1"
            }
        },
        "moderator_leet": {
            "fields": [
                "id",
                "original",
                "replacement"
            ],
            "types": {
                "id": "int(11) NOT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "original": "text NOT NULL",
                "replacement": "text NOT NULL"
            }
        },
        "moderator_warning": {
            "fields": [
                "id",
                "userid",
                "username",
                "count",
                "datetime_insert",
                "datetime_update"
            ],
            "types": {
                "id": "int(11) NOT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "userid": "varchar(30) NOT NULL",
                "username": "varchar(30) NOT NULL",
                "count": "int(11) NOT NULL DEFAULT 1",
                "datetime_insert": "datetime NOT NULL",
                "datetime_update": "datetime NOT NULL"
            }
        },
        "moderator_warning_level": {
            "fields": [
                "level",
                "action",
                "duration",
                "explanation",
                "reason"
            ],
            "types": {
                "level": "int(11) NOT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "action": "int(11) NOT NULL",
                "duration": "int(11) NOT NULL",
                "explanation": "text NOT NULL",
                "reason": "text NOT NULL"
            }
        },
        "reactions": {
            "fields": [
                "id",
                "trigger_word",
                "reaction",
                "frequency",
                "timeout",
                "tts"
            ],
            "types": {
                "id": "int(11) NOT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "trigger_word": "varchar(30) NOT NULL",
                "reaction": "text NOT NULL",
                "frequency": "tinyint(4) NOT NULL",
                "timeout": "int(11) NOT NULL",
                "tts": "BOOLEAN NOT NULL DEFAULT FALSE"
            }
        },
        "shout": {
            "fields": [
                "id",
                "original",
                "replacement",
                "language",
                "type"
            ],
            "types": {
                "id": "int(11) NOT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "original": "text NOT NULL",
                "replacement": "text NOT NULL",
                "language": "text NOT NULL",
                "type": "int(11) NOT NULL DEFAULT 0"
            }
        },
        "tanks": {
            "fields": [
                "id",
                "trigger_word",
                "nation",
                "tier",
                "name",
                "mark",
                "max_dmg",
                "note",
                "type"
            ],
            "types": {
                "id": "int(11) NOT NULL AUTO_INCREMENT FIRST, ADD PRIMARY KEY (`id`)",
                "trigger_word": "varchar(30) NOT NULL",
                "nation": "text NOT NULL",
                "tier": "tinyint(2) NOT NULL",
                "name": "text NOT NULL",
                "mark": "tinyint(1) NOT NULL",
                "max_dmg": "int(5) NOT NULL",
                "note": "text NULL",
                "type": "varchar(10) NOT NULL"
            }
        },
        "tanks_alias": {
            "fields": [
                "alias",
                "tank"
            ],
            "types": {
                "alias": "varchar(30) NOT NULL",
                "tank": "text NOT NULL"
            }
        },
        "tanks_nation": {
            "fields": [
                "alias",
                "nation"
            ],
            "types": {
                "alias": "varchar(30) NOT NULL",
                "nation": "text NOT NULL"
            }
        },
        "users": {
            "fields": [
                "username",
                "password"
            ],
            "types": {
                "username": "varchar(20) NOT NULL",
                "password": "text DEFAULT 0"
            }
        }
    },
